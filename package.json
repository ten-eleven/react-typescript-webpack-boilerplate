{
  "name": "typescript-react-boilerplate",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "postinstall": "tsd reinstall --overwrite --save; npm run-script build",
    "start": "node start.js",
    "test": "karma start --single-run",
    "build": "rm -rf dist && NODE_ENV=production node ./node_modules/webpack/bin/webpack -p --config ./webpack.prod.config.js --progress --profile --colors",
    "test-e2e":"rm -rf test/e2e/generated; tsc -p test/e2e; protractor test/e2e/config/protractor.conf.js"
  },
  "author": "Joseph McElroy <joe@teneleven.co.uk>",
  "license": "MIT",
  "dependencies": {
    "assets-webpack-plugin": "^3.1.0",
    "autoprefixer": "^6.0.3",
    "css-loader": "^0.20.2",
    "ejs": "^2.3.4",
    "express": "^4.13.3",
    "extract-text-webpack-plugin": "^0.8.2",
    "file-loader": "^0.8.4",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.14",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.1",
    "karma-webpack": "^1.7.0",
    "node-sass": "^3.3.3",
    "normalize.css": "^3.0.3",
    "null-loader": "^0.1.1",
    "phantomjs": "^1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-hot-loader": "^1.3.0",
    "sass-loader": "^3.0.0",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.5.6",
    "tsd": "^0.6.5",
    "typescript": "^1.6.2",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.2",
    "webpack-config": "^1.1.1"
  },
  "devDependencies": {
    "html-webpack-plugin": "^1.6.2",
    "webpack-dev-middleware": "^1.2.0",
    "webpack-dev-server": "^1.12.1",
    "webpack-hot-middleware": "^2.5.0",
    "xenon": "ten-eleven/xenon"
  }
}
